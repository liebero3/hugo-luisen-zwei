#content a,.nav-link{color:#aa0049!important;text-decoration:none}#content a *,.nav-link *{color:#aa0049}#content a:hover,.nav-link:hover{color:#85083e!important;text-decoration:none}#footer a,.navbar-brand{color:#4b4b4b;text-decoration:none}#footer a *,.navbar-brand *{color:#4b4b4b}#footer a:hover,.navbar-brand:hover{color:#4b4b4b;text-decoration:none}#content table td,#content table th{border:1px solid #ccc;padding:6px 12px;text-align:left}*{color:#4b4b4b;font-family:montserrat,Verdana,sans-serif;line-height:1.2}.navbar-brand{font-size:2rem}hr{margin:0}hr.luisen-yellow{margin-left:10%;margin-right:10%;border:3px solid #fc0;width:69%;margin-top:10px;margin-bottom:3px}hr.luisen-red{margin-left:10%;margin-right:10%;border:3px solid #aa0049;width:70%;margin-bottom:3px}hr.luisen-blue{margin-left:10%;margin-right:10%;border:3px solid #18aad7;width:68%;margin-bottom:10px}#content p{margin-bottom:.6rem}#content h1,#content h2,#content h3,#content h4,#content h5,#content h6{font-size:medium;font-weight:700;margin:1rem 0 .6rem}#content h1{font-size:1.8rem}#content h2{font-size:1.6rem}#content h3{font-size:1.4rem}#content h4{font-size:1.2rem}#content img{display:block;margin:1rem auto}#content .avatar>img{border-radius:50%;float:right;margin:-8px 0 0 16px;height:90px;width:90px}#content ol{counter-reset:list;list-style:none;padding-left:0}#content ol>li:before{content:"[" counter(list,decimal)"] ";counter-increment:list}#content ul{list-style:inside;padding-left:0}#content table{margin:1rem auto;width:100%}#content table th{font-weight:700}#content table tr:nth-child(2n){background-color:#f8f8f8}#content blockquote{border-left:4px solid;font-style:italic;margin:1rem 0;padding:8px}#content code{color:#4b4b4b;background-color:#f8f8f8;border:1px solid #ccc;border-radius:10%;padding:0 4px}#content pre code{all:unset}#content .highlight{margin:1rem auto}#content .highlight>pre{padding:8px}#content .card img{margin:0}#content .card h5,#content .card p{color:#4b4b4b}#content #text{